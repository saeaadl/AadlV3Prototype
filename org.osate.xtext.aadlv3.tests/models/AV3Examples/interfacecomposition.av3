	type person ;
	type address;
	interface sub is
		name : feature person;
		p1: feature date;
		p2: in port date; 
	end ;
	
	interface mine is
		name: feature person;
	end ;
	
	system interface subsub extends sub, mine
	// should have error message about duplicate names coming from the two extends elements
	end;
	
	system subsub.i is 
		c1: feature p1 -> name ;
	end ;
end ;