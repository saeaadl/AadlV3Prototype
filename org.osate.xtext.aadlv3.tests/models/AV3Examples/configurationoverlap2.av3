package configurationoverlap2 
import StandardProperties::*;

property SecurityLevel: int for all;
property SafetyLevel: int for all;

system interface x end

system x.i  is 
	xsub1: process subsubsys;
 	xsub2: process subsubsys.i ;
end

process interface subsubsys
end

process subsubsys.i end

process subsubsys.i2 end


 configuration x.config_L1 extends x.i is
  xsub1 => subsubsys.i; 
  xsub1 => subsubsys.i2 ;
end


	
end