package PackA 
	import PackA.nested.signal;
	type date ;
	type person ;
	type int;
	property Period int;
	property Size int;

	package nested  
		type signal ;
	end ;
	interface sub
	is
		name : in feature person;
		surname : in feature person ;
	end ;
	
	interface subsub 
	is
		p1 : in port signal ;
		p2 : in port date { #Size => 3; };
	
		p1#Size => 3;
	end ;
	system sub.impl 
	is
		s1 : process subsub ;
		s2 : thread subsub { #Period => 20; };
	end ;
	
	system interface gps end;
	
	system gps.i
	is
		locator : process LocatorProcess.impl;
	end ;
	process interface LocatorProcess
	is
	#Period => 20;
	end;
	
	process LocatorProcess.impl end;
	
end ;