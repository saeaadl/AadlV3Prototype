package PackA 
	import nested;
	type date ;
	type person ;
	type int;
	property Period int;
	property Size int;

	package nested  
		type signal ;
	end ;
	interface sub
	features
		name : in feature person;
		surname : in feature person ;
	end ;
	
	interface subsub 
	features
		p1 : in port nested::signal ;
		p11 : in port signal ;
		p2 : in port date { #Size => 3; };
	properties
		p1#Size => 3;
	end ;
	system sub.impl 
	subcomponents
		s1 : process subsub ;
		s2 : thread subsub { #Period => 20; };
	end ;
	
	system interface gps end;
	
	system gps.i
	subcomponents
		locator : process LocatorProcess.impl;
	end ;
	process interface LocatorProcess
	properties
	#Period => 20;
	end;
	
	process LocatorProcess.impl end;
	
end ;