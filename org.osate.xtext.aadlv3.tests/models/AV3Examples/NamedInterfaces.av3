package NamedInterfaces 
	type date; 
	type person ;

	interface sub 
	is
		name : feature person ;
		p1: in port date;
		p2: in port date;
	end;
	
	interface mine 
	is
		name: feature person;
	end;
	
	system interface subsub 
	is 
		subi: interface sub;
		minei: interface mine;
//	flows
//		c1: port subi.p1 -> minei.name; 
	end;
end;